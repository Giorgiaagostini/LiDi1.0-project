                         HPOS="886">
                  <String ID="string_r1l31"
                          HEIGHT="80"
                          WIDTH="2546"
                          VPOS="3293"
                          HPOS="886"
                          CONTENT="Ruenna. Altri per conto della sua antichità, vogliono che fusse stata pria edificata dalli discesi di Noè da suoi"/>
               </TextLine>
               <TextLine ID="r1l32"
                         BASELINE="886,3430 1015,3434 1145,3436 1274,3438 1404,3440 1533,3440 1663,3442 1792,3442 1922,3442 2051,3442 2181,3442 2310,3442 2440,3442 2569,3443 2699,3444 2828,3444 2958,3446 3087,3446 3217,3448 3346,3451 3476,3454"
                         HEIGHT="88"
                         WIDTH="2590"
                         VPOS="3387"
                         HPOS="886">
                  <String ID="string_r1l32"
                          HEIGHT="88"
                          WIDTH="2590"
                          VPOS="3387"
                          HPOS="886"
                          CONTENT="nepoti, dipoi da Thessali.⁓ Ma pria secondo dice Beroso fù colonia dedutta da Noè, quando molte città fece edi¬"/>
               </TextLine>
               <TextLine ID="r1l33"
                         BASELINE="892,3536 1021,3536 1150,3536 1279,3535 1408,3534 1537,3534 1666,3534 1795,3534 1924,3536 2053,3536 2183,3536 2312,3536 2441,3538 2570,3538 2699,3540 2828,3540 2957,3542 3086,3544 3215,3545 3344,3546 3474,3548"
                         HEIGHT="78"
                         WIDTH="2582"
