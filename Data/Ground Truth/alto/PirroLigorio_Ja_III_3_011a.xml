                         VPOS="3963"
                         HPOS="886">
                  <String ID="string_r1l38"
                          HEIGHT="80"
                          WIDTH="2556"
                          VPOS="3963"
                          HPOS="886"
                          CONTENT="et vi habitarono su' gli carri perle acque, trovando quasi tutto'l paese undoso ò Umbroso et male culto:"/>
               </TextLine>
