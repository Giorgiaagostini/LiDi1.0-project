                  <String ID="string_r1l29"
                          HEIGHT="89"
                          WIDTH="2572"
                          VPOS="3111"
                          HPOS="898"
                          CONTENT="paese molle, per che presso d'essa sono i quinque mari stagni defiumi che per diversi luoghi gli italici vi na¬"/>
               </TextLine>
               <TextLine ID="r1l30"
                         BASELINE="894,3234 1020,3237 1146,3240 1272,3243 1398,3246 1524,3248 1650,3250 1776,3251 1902,3252 2028,3254 2155,3254 2281,3254 2407,3256 2533,3256 2659,3256 2785,3254 2911,3254 3037,3254 3163,3252 3289,3252 3416,3250"
