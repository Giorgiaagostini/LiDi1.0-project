                  <String ID="string_r1l37"
                          HEIGHT="74"
                          WIDTH="2608"
                          VPOS="3873"
                          HPOS="886"
                          CONTENT="ti, Crano et Crana, che in Italia sul Tibere fiume Regnarono nella Hetruria, et nell'Umbria, che occuparono,"/>
               </TextLine>
