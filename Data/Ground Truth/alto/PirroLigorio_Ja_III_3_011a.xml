                         HPOS="894">
                  <String ID="string_r1l30"
                          HEIGHT="83"
                          WIDTH="2522"
                          VPOS="3193"
                          HPOS="894"
                          CONTENT="vigavano. Per chè anchora sendo dove concorrono et rompono i fiumi col constrasto ancho del mare ne viene detta"/>
               </TextLine>
               <TextLine ID="r1l31"
                         BASELINE="886,3336 1013,3338 1140,3340 1267,3342 1395,3342 1522,3344 1649,3346 1777,3348 1904,3348 2031,3350 2159,3350 2286,3352 2413,3352 2540,3352 2668,3352 2795,3352 2922,3352 3050,3350 3177,3350 3304,3348 3432,3344"
