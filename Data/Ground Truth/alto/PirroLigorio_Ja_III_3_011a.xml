                         HPOS="894">
                  <String ID="string_r1l36"
                          HEIGHT="76"
                          WIDTH="2606"
                          VPOS="3775"
                          HPOS="894"
                          CONTENT="che facessero alcune città in Hetruria, et da essi nacque Ravenna. Altri dicono che, che furono da Noè manda¬"/>
               </TextLine>
               <TextLine ID="r1l37"
                         BASELINE="886,3916 1016,3919 1146,3922 1277,3924 1407,3925 1538,3926 1668,3926 1798,3926 1929,3926 2059,3924 2190,3924 2320,3922 2450,3922 2581,3920 2711,3920 2842,3918 2972,3918 3102,3918 3233,3918 3363,3918 3494,3918"
