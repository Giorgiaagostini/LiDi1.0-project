                  <String ID="string_r1l41"
                          HEIGHT="84"
                          WIDTH="2602"
                          VPOS="4253"
                          HPOS="872"
                          CONTENT="sti appellati Curuli dalli Curri, ò carri che usavano. et dalli sacri misterij detti Hetrusci.⁓ et questi vogliono, che "/>
               </TextLine>
