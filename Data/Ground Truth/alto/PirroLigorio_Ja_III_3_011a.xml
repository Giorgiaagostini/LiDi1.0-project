                         HPOS="892">
                  <String ID="string_r1l33"
                          HEIGHT="78"
                          WIDTH="2582"
                          VPOS="3491"
                          HPOS="892"
                          CONTENT="ficare,nella successione doppo il Diluvio universale. et nelle edificationi vi mando Gomer, Gallo, Comer, et Ia¬"/>
               </TextLine>
               <TextLine ID="r1l34"
                         BASELINE="894,3630 1021,3630 1148,3632 1275,3632 1402,3632 1529,3634 1656,3634 1783,3634 1910,3634 2037,3634 2164,3636 2291,3636 2418,3636 2545,3636 2672,3637 2799,3638 2926,3638 3053,3640 3180,3640 3307,3642 3434,3642"
                         HEIGHT="75"
                         WIDTH="2540"
                         VPOS="3588"
                         HPOS="894">
                  <String ID="string_r1l34"
                          HEIGHT="75"
                          WIDTH="2540"
                          VPOS="3588"
                          HPOS="894"
                          CONTENT="pheto suoi figliuoli, et mando questi nella parte d'Italia che fù detta Hetruria et dipoi Tuscia. Onde i Nepoti"/>
               </TextLine>
               <TextLine ID="r1l35"
                         BASELINE="870,3728 998,3728 1126,3728 1255,3728 1383,3728 1512,3728 1640,3730 1768,3730 1897,3730 2025,3732 2154,3732 2282,3734 2410,3734 2539,3734 2667,3734 2796,3734 2924,3734 3052,3734 3181,3732 3309,3730 3438,3728"
                         HEIGHT="70"
                         WIDTH="2568"
                         VPOS="3685"
                         HPOS="870">
                  <String ID="string_r1l35"
                          HEIGHT="70"
                          WIDTH="2568"
                          VPOS="3685"
                          HPOS="870"
                          CONTENT="suoi nati di Iapheto, dipoi mando nell'Armenia, et d'indigli conduste in Italia, cosi è la oppenione di Beroso, et"/>
               </TextLine>
               <TextLine ID="r1l36"
                         BASELINE="894,3818 1024,3820 1154,3822 1284,3824 1415,3824 1545,3826 1675,3826 1806,3828 1936,3828 2066,3828 2197,3830 2327,3830 2457,3830 2587,3830 2718,3830 2848,3830 2978,3830 3109,3830 3239,3830 3369,3830 3500,3830"
