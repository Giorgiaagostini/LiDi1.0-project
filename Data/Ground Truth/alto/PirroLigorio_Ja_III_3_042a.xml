                         WIDTH="2522"
                         VPOS="3635"
                         HPOS="934">
                  <String ID="string_r1l37"
                          HEIGHT="95"
                          WIDTH="2522"
                          VPOS="3635"
                          HPOS="934"
                          CONTENT="tempeste exagitati, havere molti luoghi del mare errato. et allultimo esser inquel luogo dell'opi¬"/>
               </TextLine>
               <TextLine ID="r1l38"
                         BASELINE="922,3786 1046,3786 1171,3786 1296,3786 1421,3788 1546,3788 1671,3788 1796,3788 1921,3788 2046,3788 2171,3788 2295,3788 2420,3788 2545,3788 2670,3790 2795,3790 2920,3790 3045,3790 3170,3791 3295,3792 3420,3792"
                         HEIGHT="69"
                         WIDTH="2498"
                         VPOS="3744"
                         HPOS="922">
                  <String ID="string_r1l38"
                          HEIGHT="69"
                          WIDTH="2498"
                          VPOS="3744"
                          HPOS="922"
                          CONTENT="ca Regione venuti, che vicino al Mare Tyrrheno latino chiamavasi, et conlieti animi, veduto"/>
               </TextLine>
               <TextLine ID="r1l39"
                         BASELINE="924,3884 1049,3889 1175,3893 1301,3896 1426,3896 1552,3902 1678,3902 1804,3905 1929,3892 2055,3890 2181,3888 2306,3886 2432,3884 2558,3882 2683,3882 2809,3882 2935,3882 3060,3884 3186,3888 3312,3894 3473,3903"
