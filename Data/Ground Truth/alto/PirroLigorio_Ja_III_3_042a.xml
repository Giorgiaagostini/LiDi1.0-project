                         HEIGHT="81"
                         WIDTH="2492"
                         VPOS="3942"
                         HPOS="918">
                  <String ID="string_r1l40"
                          HEIGHT="81"
                          WIDTH="2492"
                          VPOS="3942"
                          HPOS="918"
                          CONTENT="mente all'entrar' della prima vera di seguir loro corso: ma abbruciate lor di notte Le Navi,"/>
               </TextLine>
               <TextLine ID="r1l41"
                         BASELINE="920,4088 1045,4094 1170,4100 1295,4103 1420,4104 1546,4106 1671,4104 1796,4104 1921,4102 2046,4100 2172,4098 2297,4096 2422,4093 2547,4091 2672,4090 2798,4088 2923,4088 3048,4088 3173,4090 3298,4094 3424,4098"
