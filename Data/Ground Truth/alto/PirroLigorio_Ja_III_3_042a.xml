                         WIDTH="2510"
                         VPOS="3557"
                         HPOS="930">
                  <String ID="string_r1l36"
                          HEIGHT="79"
                          WIDTH="2510"
                          VPOS="3557"
                          HPOS="930"
                          CONTENT="nysio: I' greci partiti da Troia lungo la Melea in Navi sendo presi dal mal tempo, et dale gravi"/>
               </TextLine>
               <TextLine ID="r1l37"
                         BASELINE="934,3672 1060,3676 1186,3679 1312,3682 1438,3684 1564,3686 1690,3689 1816,3691 1942,3694 2068,3696 2195,3696 2321,3698 2447,3700 2573,3702 2699,3704 2825,3704 2951,3706 3077,3708 3203,3709 3329,3710 3456,3712"
