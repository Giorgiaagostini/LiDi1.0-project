                         HPOS="906">
                  <String ID="string_r1l35"
                          HEIGHT="97"
                          WIDTH="2578"
                          VPOS="3462"
                          HPOS="906"
                          CONTENT="Damaste Sigeo. Ma Aristotele stagirita philosopho, racconta il fatto cosi presso dell'istesso Dio"/>
               </TextLine>
               <TextLine ID="r1l36"
                         BASELINE="930,3596 1055,3596 1181,3594 1306,3596 1432,3596 1557,3596 1683,3598 1808,3600 1934,3602 2059,3604 2185,3606 2310,3606 2436,3608 2561,3610 2687,3612 2812,3614 2938,3616 3063,3616 3189,3617 3314,3618 3440,3618"
