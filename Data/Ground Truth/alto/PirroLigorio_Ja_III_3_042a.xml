                         HPOS="942">
                  <String ID="string_r1l32"
                          HEIGHT="77"
                          WIDTH="2512"
                          VPOS="3174"
                          HPOS="942"
                          CONTENT="una delle Ilionense chiamata Roma seco: et costei havendo per male di andar cosi erranti per"/>
               </TextLine>
               <TextLine ID="r1l33"
                         BASELINE="936,3308 1062,3314 1189,3319 1316,3322 1443,3326 1570,3328 1696,3329 1823,3330 1950,3330 2077,3330 2204,3328 2330,3328 2457,3326 2584,3324 2711,3322 2838,3321 2964,3320 3091,3318 3218,3318 3345,3324 3472,3324"
