                          HPOS="928"
                          CONTENT="larono i sacerdoti di Argo: havendo notate le cose fatte in detto tempo, et dissero che Aenea venuto"/>
               </TextLine>
               <TextLine ID="r1l31"
                         BASELINE="934,3112 1060,3115 1186,3118 1312,3120 1438,3120 1564,3122 1690,3120 1816,3120 1942,3120 2068,3118 2194,3118 2320,3116 2446,3116 2572,3114 2698,3114 2824,3114 2950,3116 3076,3116 3202,3120 3328,3122 3454,3128"
                         HEIGHT="79"
                         WIDTH="2520"
                         VPOS="3070"
                         HPOS="934">
                  <String ID="string_r1l31"
                          HEIGHT="79"
                          WIDTH="2520"
                          VPOS="3070"
                          HPOS="934"
                          CONTENT="nelli Molossi popoli dell'Epiro doppo Ulysse in Italia. fu di Roma edificatore, per che havendo da"/>
               </TextLine>
               <TextLine ID="r1l32"
                         BASELINE="942,3216 1067,3221 1193,3224 1318,3226 1444,3228 1570,3228 1695,3228 1821,3226 1946,3226 2072,3224 2198,3224 2323,3222 2449,3220 2574,3220 2700,3220 2826,3220 2951,3220 3077,3222 3202,3223 3328,3226 3454,3230"
