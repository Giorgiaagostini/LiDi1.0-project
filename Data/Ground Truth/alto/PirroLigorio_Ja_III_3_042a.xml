                         WIDTH="2514"
                         VPOS="3840"
                         HPOS="924">
                  <String ID="string_r1l39"
                          HEIGHT="81"
                          WIDTH="2514"
                          VPOS="3840"
                          HPOS="924"
                          CONTENT="la Terra, havervi le Navi tirate, et quivi quella invernata fermatisi, apparecchiati certa¬"/>
               </TextLine>
