                         WIDTH="2536"
                         VPOS="3266"
                         HPOS="936">
                  <String ID="string_r1l33"
                          HEIGHT="85"
                          WIDTH="2536"
                          VPOS="3266"
                          HPOS="936"
                          CONTENT="mare, confortò l'altre Donne troiane ad abruciarle Navi, et cosi fecero, et cosi remaste nelluo"/>
               </TextLine>
               <TextLine ID="r1l34"
                         BASELINE="920,3404 1046,3407 1172,3410 1298,3412 1424,3414 1550,3414 1676,3416 1802,3417 1928,3418 2054,3418 2180,3420 2306,3420 2432,3420 2558,3422 2684,3422 2810,3422 2936,3424 3062,3424 3188,3426 3314,3428 3440,3430"
                         HEIGHT="89"
                         WIDTH="2520"
                         VPOS="3362"
                         HPOS="920">
                  <String ID="string_r1l34"
                          HEIGHT="89"
                          WIDTH="2520"
                          VPOS="3362"
                          HPOS="920"
                          CONTENT="go dove si fermarono fu fatta Roma et da lei appellata: et questa istessa gloria gli da nelli suoi scritti"/>
               </TextLine>
               <TextLine ID="r1l35"
                         BASELINE="906,3504 1034,3506 1163,3506 1292,3508 1421,3509 1550,3510 1679,3512 1808,3513 1937,3514 2066,3516 2195,3518 2323,3518 2452,3520 2581,3522 2710,3524 2839,3526 2968,3528 3097,3530 3226,3532 3355,3547 3474,3560"
