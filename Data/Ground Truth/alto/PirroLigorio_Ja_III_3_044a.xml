                  <String ID="string_r1l33"
                          HEIGHT="131"
                          WIDTH="2446"
                          VPOS="2982"
                          HPOS="952"
                          CONTENT="si, et di grandezza et di signoria, et di suntuosissimi, et miracolosissimi edificij, piena d'ogni nobilta, et"/>
               </TextLine>
               <TextLine ID="r1l34"
                         BASELINE="950,3104 1072,3110 1195,3116 1318,3122 1441,3126 1564,3132 1687,3138 1810,3143 1933,3148 2056,3152 2179,3157 2301,3161 2424,3164 2547,3168 2670,3172 2793,3175 2916,3178 3039,3180 3162,3182 3285,3184 3408,3186"
