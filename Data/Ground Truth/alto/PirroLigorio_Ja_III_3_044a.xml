                  <String ID="string_r1l32"
                          HEIGHT="121"
                          WIDTH="2430"
                          VPOS="2908"
                          HPOS="966"
                          CONTENT="pra ogni altra città facendosi padrona et famosa, et piena di fatti egregij, di huomini forti et virtuo¬"/>
               </TextLine>
               <TextLine ID="r1l33"
                         BASELINE="952,3020 1074,3028 1196,3034 1318,3040 1441,3046 1563,3050 1685,3056 1808,3060 1930,3064 2052,3068 2175,3071 2297,3074 2419,3077 2541,3080 2664,3082 2786,3084 2908,3086 3031,3089 3153,3090 3275,3092 3398,3094"
