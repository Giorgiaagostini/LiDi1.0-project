                          HEIGHT="137"
                          WIDTH="2458"
                          VPOS="3067"
                          HPOS="950"
                          CONTENT="madre ditutti divenne universalmente, et di tutte le cose eccellenti dell'arti delle ricchezze, delle cose"/>
               </TextLine>
               <TextLine ID="r1l35"
                         BASELINE="968,3180 1088,3187 1208,3193 1328,3199 1448,3205 1568,3210 1688,3216 1808,3220 1928,3224 2048,3228 2168,3232 2288,3236 2408,3240 2528,3242 2648,3246 2768,3250 2888,3253 3008,3256 3128,3260 3248,3264 3368,3268"
